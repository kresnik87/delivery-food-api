{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^3.2",
    "aws/aws-sdk-php": "^3.257",
    "doctrine/annotations": "^1.13",
    "doctrine/dbal": "3.*",
    "doctrine/doctrine-bundle": "^2.6",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.12",
    "dragonmantank/cron-expression": "^3.3",
    "easycorp/easyadmin-bundle": "^4.8",
    "fakerphp/faker": "^1.21",
    "gedmo/doctrine-extensions": "^3.10",
    "justinrainbow/json-schema": "^5.2",
    "lambdish/phunctional": "^2.1",
    "league/flysystem-aws-s3-v3": "^3.12",
    "lexik/jwt-authentication-bundle": "^2.16",
    "nelmio/cors-bundle": "^2.2",
    "oneup/flysystem-bundle": "^4.6",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.5",
    "ramsey/uuid": "^4.6",
    "sendgrid/sendgrid": "^8.0",
    "sensio/framework-extra-bundle": "6.2.9",
    "specshaper/encrypt-bundle": "dev-master",
    "symfony/amqp-messenger": "6.3.*",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "6.3.*",
    "symfony/console": "*",
    "symfony/doctrine-messenger": "6.3.*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "6.3.*",
    "symfony/flex": "^1.17|^2",
    "symfony/form": "6.3.*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "6.3.*",
    "symfony/mailer": "6.3.*",
    "symfony/mime": "6.3.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "6.3.*",
    "symfony/process": "6.3.*",
    "symfony/property-access": "6.3.*",
    "symfony/property-info": "6.3.*",
    "symfony/proxy-manager-bridge": "6.3.*",
    "symfony/runtime": "*",
    "symfony/scheduler": "6.3.*",
    "symfony/security-bundle": "6.3.*",
    "symfony/serializer": "6.3.*",
    "symfony/string": "6.3.*",
    "symfony/translation": "6.3.*",
    "symfony/twig-bundle": "6.3.*",
    "symfony/uid": "6.3.*",
    "symfony/validator": "6.3.*",
    "symfony/web-link": "6.3.*",
    "symfony/webpack-encore-bundle": "^1.12",
    "symfony/workflow": "6.3.*",
    "symfony/yaml": "*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "vich/uploader-bundle": "^2.0"
  },
  "require-dev": {
    "beberlei/assert": "^3.3",
    "hautelook/alice-bundle": "^2.11",
    "phpstan/extension-installer": "1.1.0",
    "phpstan/phpstan": "^1.7",
    "phpstan/phpstan-doctrine": "1.3.9",
    "phpstan/phpstan-symfony": "1.2.5",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "6.3.*",
    "symfony/css-selector": "6.3.*",
    "symfony/debug-bundle": "6.3.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "6.3.*",
    "symfony/stopwatch": "6.3.*",
    "symfony/var-dumper": "6.3.*",
    "symfony/web-profiler-bundle": "6.3.*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true,
      "ocramius/package-versions": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "KsK\\Apps\\Delivery\\": "apps/delivery-api/src/",
      "DoctrineMigrations\\": "src/Delivery/Infrastructure/Shared/Migrations",
      "KsK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KsK\\Tests\\": [
        "tests"
      ]
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon src tests --level max --no-progress",
    "tests": "phpunit --configuration phpunit.xml.dist",
    "ci": [
      "@phpstan",
      "@tests"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install apps/delivery-api/public": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.3.*",
      "docker": true
    },
    "bin-dir": "apps/delivery-api/bin"
  }
}
